      "url": "/exit",
      "icons": [{ "src": "/icon-light-32x32.png", "sizes": "32x32" }]
    }
  ],
  "share_target": {
    "action": "/entry",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "image",
          "accept": ["image/*"]
        }
      ]
    }
  }
